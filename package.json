{
  "name": "minimal-react-components",
  "version": "2.2.1",
  "author": "Vinay Mehta",
  "description": "Collection of basic react components for your app.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:vinaymehta1860/minimal-react-components.git"
  },
  "keywords": [
    "react",
    "components",
    "ui"
  ],
  "files": [
    "index.js",
    "build/*"
  ],
  "scripts": {
    "pub": "npm publish --access=public",
    "start": "webpack --watch",
    "build": "rm -rf build/* && webpack",
    "clean": "rm -rf node_modules package-lock.json",
    "clean-cache": "npm cache clean --force",
    "clean-setup": "npm run clean && npm run clean-cache && npm install",
    "lint": "eslint components/*",
    "build-and-publish": "npm run build && npm run publish"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "^7.29.4",
    "ionicons": "^6.0.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "styled-components": "^5.3.3",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "webpack": "^5.72.0"
  }
}
